{
  "name": "tvds-server",
  "version": "1.0.0",
  "description": "",
  "types": "dist/src/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "devStart": "nodemon script.ts",
    "dev": "ts-node-dev src/index.ts",
    "start": "node .",
    "gen": "npx prisma generate && sh gen.sh",
    "prebuild": "npm run gen",
    "build": "webpack && bytenode -c dist/index.js -l index.js",
    "build:types": "tsc --declaration --noEmit",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "db:init": "prisma migrate dev",
    "lint": "eslint --fix **/*.ts && tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@prisma/client": "^4.16.2",
    "@trpc/server": "10.19.1",
    "@ts-rest/core": "^3.26.3",
    "@ts-rest/express": "^3.26.3",
    "argon2": "^0.30.3",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "bytenode": "^1.4.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.2",
    "jsonwebtoken": "^9.0.0",
    "ms": "^2.1.3",
    "pino": "^8.14.1",
    "rxjs": "^7.8.0",
    "superjson": "^1.12.4",
    "trpc-panel": "^1.3.4",
    "trpc-playground": "^1.0.4",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/google-protobuf": "^3.15.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
